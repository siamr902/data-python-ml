      "source": [
        "Time taken by list vs. numpy array"
      ],
      "metadata": {
        "id": "GMRXT5Tuu3bB"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "python_list = [x for x in range(10000)]\n",
        "start_time = process_time()\n",
        "\n",
        "python_list = [n + 5 for n in python_list]\n",
        "end_time = process_time()\n",
        "\n",
        "print(end_time - start_time)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
